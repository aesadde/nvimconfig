{
  "suggest.maxCompleteItemCount": 10,
  "coc.preferences.formatOnSaveFiletypes": ["go"],
  "coc.preferences.jumpCommand": "split",
  "coc.preferences.codeLens.enable": true,
  "diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "signature.target": "echo",
  "languageserver": {
    "golang": {
      "command": "go-langserver",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    },
    "bash": {
      "command": "bash-language-server",
      "filetypes": ["sh"],
      "args": ["start"],
      "ignoredRootPaths": ["~"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {}
    }
  },
  "java.format.settings.url": "/Users/aesadde/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.format.comments.enabled": true,
  "java.import.gradle.wrapper.enabled": true,
  "java.autobuild.enabled": true
}
