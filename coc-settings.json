{
	"suggest.maxCompleteItemCount": 5,
	"coc.preferences.formatOnSaveFiletypes": ["go"],
	"coc.preferences.jumpCommand": "split",
	"diagnostic.virtualText": true,
	"diagnostic.checkCurrentLine": true,
	"signature.target": "echo",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
			"filetypes": ["go"]
		},
		"bash": {
			"command": "bash-language-server",
			"filetypes": ["sh"],
			"args": ["start"],
			"ignoredRootPaths": ["~"]
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		},
		"haskell": {
			"command": "hie-wrapper",
			"rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
			"filetypes": ["hs", "lhs", "haskell"],
			"initializationOptions": {}
		}
	},
	"java.format.settings.url": "/Users/aesadde/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
	"java.format.comments.enabled": true,
	"java.import.gradle.wrapper.enabled": true,
	"java.autobuild.enabled": true,
  "yaml.hover": false,
  "yaml.validate": false
}
